  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@rocketseat/eslint-config": "^2.2.2",
    "@swc/core": "^1.6.3",
    "@swc/jest": "^0.2.36",
